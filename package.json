{
  "name": "@coderebus/react-hooks",
  "description": "A collection of custom React hooks",
  "version": "1.3.0",
  "keywords": [
    "useDebounce",
    "useLocalStorage",
    "useSessionStorage",
    "useThrottle",
    "useIntersection",
    "useLocalizedContent",
    "useFetch",
    "react",
    "hooks"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "bump": "bump",
    "pub": "npm run bump && npm run build && cd dist && npm publish"
  },
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.8.5",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "eslint": "^8.43.0",
    "glob": "^10.3.10",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tsup": "7.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
